<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nick">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SignIN.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIN.SignInActivity" />
        <activity android:name=".Register.RegisterActivity" />
        <activity android:name=".SignIN.LogInActivity" />
        <activity android:name=".Themes.ThemeActivity" />
        <activity android:name=".ChatBot.ChatBotActivity" />
        <activity android:name=".Interest.InterestActivity"/>
        <activity android:name=".Information.InformationActivity"/>

    </application>

</manifest>